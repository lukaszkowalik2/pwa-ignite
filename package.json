{
	"name": "pwa-ignite",
	"version": "0.4.0",
	"description": "Kickstart your Progressive Web App development with a blazing-fast setup. PWA Ignite simplifies the process, making it easier than ever to launch your PWA.",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/lukaszkowalik2/pwa-ignite.git"
	},
	"license": "MIT",
	"author": {
		"email": "lkowalik04@gmail.com"
	},
	"type": "module",
	"main": "./lib/pwa.global.js",
	"module": "./lib/pwa.js",
	"types": "./lib/pwa.d.ts",
	"files": [
		"lib/",
		"package.json",
		"LICENSE.md",
		"README.md"
	],
	"scripts": {
		"build": "tsup",
		"format": "prettier .",
		"lint": "eslint . --max-warnings 0",
		"lint:knip": "knip",
		"lint:md": "markdownlint \"**/*.md\" \".github/**/*.md\" --rules sentences-per-line",
		"lint:packages": "pnpm dedupe --check",
		"lint:spelling": "cspell \"**\" \".github/**/*\"",
		"prepare": "husky",
		"test": "vitest",
		"tsc": "tsc",
		"watch": "tsup --watch"
	},
	"lint-staged": {
		"*": "prettier --ignore-unknown --write"
	},
	"devDependencies": {
		"@eslint-community/eslint-plugin-eslint-comments": "4.4.0",
		"@eslint/js": "9.11.1",
		"@release-it/conventional-changelog": "8.0.1",
		"@types/eslint-plugin-markdown": "2.0.2",
		"@types/eslint__js": "8.42.3",
		"@types/node": "22.6.1",
		"@types/web-app-manifest": "1.0.8",
		"@vitest/coverage-v8": "2.0.5",
		"@vitest/eslint-plugin": "1.1.0",
		"console-fail-test": "0.5.0",
		"cspell": "8.14.2",
		"eslint": "9.11.1",
		"eslint-plugin-jsdoc": "50.2.2",
		"eslint-plugin-jsonc": "2.16.0",
		"eslint-plugin-markdown": "5.1.0",
		"eslint-plugin-n": "17.10.2",
		"eslint-plugin-package-json": "0.15.2",
		"eslint-plugin-perfectionist": "3.7.0",
		"eslint-plugin-regexp": "2.6.0",
		"eslint-plugin-yml": "1.14.0",
		"goober": "2.1.14",
		"husky": "9.1.5",
		"i18next": "23.15.1",
		"i18next-browser-languagedetector": "8.0.0",
		"jsonc-eslint-parser": "2.4.0",
		"knip": "5.30.5",
		"lint-staged": "15.2.9",
		"lucide-preact": "0.445.0",
		"markdownlint": "0.35.0",
		"markdownlint-cli": "0.42.0",
		"preact": "10.23.2",
		"prettier": "3.3.3",
		"prettier-plugin-curly": "0.2.2",
		"prettier-plugin-packagejson": "2.5.2",
		"prettier-plugin-sh": "0.14.0",
		"release-it": "17.6.0",
		"sentences-per-line": "0.2.1",
		"tsup": "8.2.4",
		"type-fest": "4.26.0",
		"typescript": "5.6.2",
		"typescript-eslint": "8.7.0",
		"vitest": "2.0.5"
	},
	"engines": {
		"node": ">=18.3.0"
	},
	"publishConfig": {
		"provenance": true
	}
}
